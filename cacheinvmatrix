makeCacheMatrix <- function(x = matrix()){
tap <- NULL
set <- function(y){
x <<- y
tap <<- NULL
}
get <-function() {X}
setInverse <- function(inverse) {tap <<- inverse}
getInverse <- function() {tap}
list(set = set, get = get, setInverse = setInverse, getInverse = getInverse)
}

cacheSolve <- function(x, ...){
tap <-x$getInverse()
if(!is.null(tap)){
message("getting cached data")
return(tap)
}
mat <- x$get()
tap <- solve(mat, ...)
x$setInverse(tap)
tap
}
